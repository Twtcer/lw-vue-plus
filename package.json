{
  "name": "@landwind/lw-vue-plus",
  "description": "基于vue3和element-plus的二次封装组件、文档",
  "author": "landwind",
  "private": false,
  "version": "0.0.1",
  "main": "lib/lw-vue-plus.umd.cjs",
  "module": "lib/lw-vue-plus.umd.cjs",
  "types": "lib/packages/index.d.ts",
  "type": "module",
  "license": "MIT",
  "files": [
    "package.json",
    "README.md",
    "LICENSE",
    "lib",
    "global.d.ts"
  ],
  "scripts": {
    "lib-all": "rimraf lib && run-p type-check build:es build:browser",
    "lib": "rimraf lib && vite build",
    "build:es": "vite build",
    "build:browser": "vite -f iife build",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "type-check": "vue-tsc --noEmit",
    "prettier:lib": "prettier --write \"packages/**/*.{js,ts,json,tsx,css,less,scss,vue,html,md}\"",
    "prettier:docs": "prettier --write \"docs/**/*.{js,ts,json,tsx,css,less,scss,vue,html,md}\"",
    "npm:publish": "npm publish --access public"
  },
  "devDependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "@rollup/plugin-replace": "^5.0.5",
    "@rollup/plugin-terser": "^0.4.4",
    "@types/node": "^22.1.0",
    "@types/nprogress": "^0.2.3",
    "@types/path-browserify": "^1.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.1",
    "@typescript-eslint/parser": "^8.0.1",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vitejs/plugin-vue-jsx": "^4.0.0",
    "@vue/shared": "^3.3.13",
    "@vueuse/core": "^10.11.0",
    "axios": "^1.7.3",
    "chalk": "^5.3.0",
    "consola": "^3.2.3",
    "echarts": "^5.5.1",
    "element-plus": "^2.7.8",
    "escape-html": "^1.0.3",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-vue": "^9.27.0",
    "jsencrypt": "3.0.0-rc.1",
    "markdown-it": "^13.0.1",
    "markdown-it-container": "^3.0.0",
    "minimist": "^1.2.8",
    "normalize.css": "^8.0.1",
    "npm-run-all": "^4.1.5",
    "pinia": "^2.2.1",
    "prettier": "^3.3.3",
    "sass": "^1.77.8",
    "sass-loader": "^16.0.0",
    "sortablejs": "^1.15.0",
    "typescript": "^5.2.2",
    "unocss": "^0.51.13",
    "unplugin-auto-import": "^0.11.2",
    "vite": "^5.3.4",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-dts": "^1.7.3",
    "vite-plugin-vue-setup-extend": "^0.4.0",
    "vitepress": "^1.3.2", 
    "vue": "^3.4.31",
    "vue-tsc": "^2.0.24",
    "vuedraggable": "^4.1.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Twtcer/lw-vue-plus.git"
  },
  "homepage": "https://Twtcer.github.io/lw-vue-plus",
  "keywords": [
    "element-plus",
    "vue",
    "vite",
    "vitepress",
    "lw-vue-plus",
    "landwind",
    "components",
    "custom-components"
  ]
}