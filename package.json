{
  "name": "lw-vue-plus",
  "description": "基于vue3和element-plus的封装组件、文档",
  "author": "landwind",
  "private": false,
  "version": "0.0.4",
  "main": "index.js",
  "type": "module",
  "license": "MIT",
  "workspaces": [
    "packages/*"
  ],
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,vue}": [
      "eslint --ext .js,.jsx,.vue,.ts,.tsx --fix --quiet ./",
      "stylelint --fix \"packages/components/src/**/*.{css,less}\""
    ]
  },
  "scripts": {
    "docs:dev": "pnpm run --filter site docs:dev",
    "docs:build": "pnpm run --filter site docs:build",
    "docs:deploy": "pnpm run --filter site docs:deploy",
    "build:components": "gulp -f packages/components/script/build/index.ts & pnpm run --filter ./packages/utils build",
    "publish:components": "gulp -f packages/components/script/publish/index.ts",
    "publish:utils": "gulp -f packages/utils/publish/index.ts",
    "play:dev": "pnpm run --filter ./play dev",
    "lint:script": "eslint --ext .js,.jsx,.vue,.ts,.tsx --fix --quiet ./",
    "lint:style": "stylelint --fix \"packages/components/src/**/*.{css,less}\"",
    "prepare": "husky install",
    "lint-staged": "lint-staged",
    "type-check": "vue-tsc --noEmit"
  },
  "devDependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "@landwind/lw-ui-plus": "workspace:*",
    "@landwind/utils": "workspace:*",
    "@rollup/plugin-replace": "^5.0.5",
    "@rollup/plugin-terser": "^0.4.4",
    "@types/gulp": "^4.0.17",
    "@types/node": "^22.1.0",
    "@types/path-browserify": "^1.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.1",
    "@typescript-eslint/parser": "^8.0.1",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vitejs/plugin-vue-jsx": "^4.0.0",
    "@vue/shared": "^3.3.13",
    "@vueuse/core": "^10.11.0",
    "axios": "^1.7.3",
    "chalk": "^5.3.0",
    "consola": "^3.2.3",
    "echarts": "^5.5.1",
    "escape-html": "^1.0.3",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-vue": "^9.27.0",
    "gulp": "^5.0.0",
    "gulp-autoprefixer": "^9.0.0",
    "gulp-less": "^5.0.0",
    "jsencrypt": "3.0.0-rc.1",
    "markdown-it": "^13.0.1",
    "markdown-it-container": "^3.0.0",
    "minimist": "^1.2.8",
    "normalize.css": "^8.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.3.3",
    "release-it": "^17.6.0",
    "sass": "^1.77.8",
    "sass-loader": "^16.0.0",
    "sortablejs": "^1.15.0",
    "typescript": "^5.2.2",
    "unplugin-auto-import": "^0.11.2",
    "vite": "^5.3.4",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-dts": "^1.7.3",
    "vite-plugin-vue-setup-extend": "^0.4.0",
    "vitepress": "^1.3.2",
    "vue": "^3.4.31",
    "vue-tsc": "^2.0.24",
    "vuedraggable": "^4.1.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Twtcer/lw-vue-plus.git"
  },
  "homepage": "https://Twtcer.github.io/lw-vue-plus",
  "keywords": [
    "element-plus",
    "vue",
    "vite",
    "vitepress",
    "lw-vue-plus",
    "landwind",
    "components",
    "custom-components"
  ]
}